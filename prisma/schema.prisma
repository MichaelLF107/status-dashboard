generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Traffic {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  method    String
  path      String
  status    Int
  duration  Int
}

model Performance {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  loadAvg   Float
  memUsed   Float
  memFree   Float
  cpuUsed   Float
  cpuFree   Float
}

model UsersStatus {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  users     Int
  guests    Int
  bots      Int
}